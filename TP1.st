Object subclass: #AlgoRastreadorZ	instanceVariableNames: 'criterio registro'	classVariableNames: ''	package: 'TP1'!!AlgoRastreadorZ methodsFor: 'as yet unclassified' stamp: 'MartinAlderete 5/9/2024 16:06'!registrarRastreoConModelo: unModelo DelPeleadorConNombre: unNombre KiBase: unKiBase yTransformacion: unaTransformacion	| peleadorNuevo |		peleadorNuevo := (Peleador conNombre:  unNombre kiBase: unKiBase  transformacion:  unaTransformacion yModelo: unModelo).		registro add: peleadorNuevo.! !!AlgoRastreadorZ methodsFor: 'initialization' stamp: 'MartinAlderete 5/8/2024 13:37'!initialize	registro := OrderedCollection new.! !!AlgoRastreadorZ methodsFor: 'not defined category' stamp: 'MartinAlderete 5/9/2024 17:49'!buscarPeleadorPorNombre: unNombre	|peleadorBuscado|		peleadorBuscado := registro detect: [ :peleador | peleador nombre  = unNombre ]			ifNone: [PeleadorNoEncontradoError signal].				^ peleadorBuscado.! !!AlgoRastreadorZ methodsFor: 'not defined category' stamp: 'MartinAlderete 5/8/2024 16:42'!nivelDePeleaDe: unNombre		^ ((self buscarPeleadorPorNombre: unNombre) ki).! !!AlgoRastreadorZ methodsFor: 'not defined category' stamp: 'MartinAlderete 5/9/2024 13:54'!establecerCriterio: unCriterio	|criterioFuerte criterioDebil|		criterioFuerte := 'Fuerte'.	criterioDebil := 'Debil'.		criterio :=  ((unCriterio = criterioFuerte) | (unCriterio = criterioDebil))		ifFalse:[	CriterioNoValidoError signal]		ifTrue: [ (unCriterio = criterioFuerte)			ifTrue: [ Fuerte new ] 			ifFalse: [ Debil new ]		].! !!AlgoRastreadorZ methodsFor: 'not defined category' stamp: 'MartinAlderete 5/9/2024 11:40'!obtenerPeleadorSegunCriterio	| peleadorMas |		peleadorMas := (registro isEmpty)		ifTrue: [ RegistroVacioError signal]		ifFalse: [ criterio buscarPeleadorMas: registro ].			^ peleadorMas nombre.			! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!AlgoRastreadorZ class	instanceVariableNames: ''!!AlgoRastreadorZ class methodsFor: 'not defined category' stamp: 'MartinAlderete 5/8/2024 15:22'!conCriterioMas: unCriterio	| rastreador |		rastreador := self new.		rastreador establecerCriterio: unCriterio.		^ rastreador.! !Object subclass: #Base	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Base methodsFor: 'not defined category' stamp: 'MartinAlderete 5/8/2024 14:58'!calcularKiSegunTransformacion: unKiBase	^ unKiBase.! !Error subclass: #CriterioNoValidoError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #Debil	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Debil methodsFor: 'not defined category' stamp: 'MartinAlderete 5/9/2024 11:41'!buscarPeleadorMas: unRegistro	^ (unRegistro detectMin: [:peleador | peleador ki]).! !Object subclass: #Fuerte	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Fuerte methodsFor: 'not defined category' stamp: 'MartinAlderete 5/9/2024 11:41'!buscarPeleadorMas: unRegistro	^ (unRegistro detectMax: [:peleador | peleador ki]).! !Object subclass: #KaioKen	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!KaioKen methodsFor: 'not defined category' stamp: 'MartinAlderete 5/8/2024 14:59'!calcularKiSegunTransformacion: unKiBase	^ (2 * unKiBase).! !Error subclass: #KiNoValidoError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Error subclass: #ModeloNoValidoError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #MonoGigante	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!MonoGigante methodsFor: 'not defined category' stamp: 'MartinAlderete 5/8/2024 14:59'!calcularKiSegunTransformacion: unKiBase	^ ((unKiBase * 10) asLargerPowerOfTwo).! !Object subclass: #Peleador	instanceVariableNames: 'nombre kiBase transformacion modelo'	classVariableNames: ''	package: 'TP1'!!Peleador methodsFor: 'accessing' stamp: 'MartinAlderete 5/8/2024 17:02'!nombre 	^ nombre! !!Peleador methodsFor: 'not defined category' stamp: 'MartinAlderete 5/8/2024 17:02'!ki	^ (modelo calcularKiSegunModelo: (transformacion calcularKiSegunTransformacion: kiBase))! !!Peleador methodsFor: 'not defined category' stamp: 'MartinAlderete 5/8/2024 14:27'!establecerModelo: unModelo	|visorViejo visorNuevo|		visorViejo := 'Viejo'.	visorNuevo := 'Nuevo'.		modelo := ((unModelo = visorViejo) | (unModelo = visorNuevo))		ifFalse: [ ModeloNoValidoError signal  ]		ifTrue: [ (unModelo = visorViejo) 				ifTrue: [VisorViejo new]				ifFalse: [VisorNuevo new]				]! !!Peleador methodsFor: 'not defined category' stamp: 'MartinAlderete 5/9/2024 16:21'!establecerKiBase: unKiBase	|kiMinimo|		kiMinimo := 0.		kiBase := (unKiBase < kiMinimo)		ifTrue: [KiNoValidoError signal]		ifFalse: [ unKiBase ].! !!Peleador methodsFor: 'not defined category' stamp: 'MartinAlderete 5/8/2024 14:23'!establecerTransformacion: unaTransformacion	|base kaioKen monoGigante|		base := 'Base'.	kaioKen := 'Kaio-Ken'.	monoGigante := 'Mono Gigante'.		transformacion := (((unaTransformacion = base) | (unaTransformacion = kaioKen )) | (unaTransformacion = monoGigante ))		ifFalse: [ TransformacionNoValidaError signal ]		ifTrue: [ (unaTransformacion = base)			ifTrue: [Base new]			ifFalse: [(unaTransformacion = kaioKen )					ifTrue: [KaioKen new]					ifFalse: [MonoGigante new]					]				].! !!Peleador methodsFor: 'initialization' stamp: 'MartinAlderete 5/8/2024 17:03'!initialize	nombre := ''.	kiBase := 0.	transformacion := ''.	modelo := ''.! !!Peleador methodsFor: 'as yet unclassified' stamp: 'MartinAlderete 5/8/2024 14:16'!establecerNombre: unNombre	nombre := unNombre.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Peleador class	instanceVariableNames: ''!!Peleador class methodsFor: 'not defined category' stamp: 'MartinAlderete 5/8/2024 17:21'!conNombre: unNombre kiBase: unKiBase transformacion: unaTransformacion yModelo: unModelo	|peleador|		peleador := self new.	peleador establecerNombre: unNombre.	peleador establecerKiBase: unKiBase.	peleador establecerTransformacion: unaTransformacion.	peleador establecerModelo: unModelo.		^ peleador.! !Error subclass: #PeleadorNoEncontradoError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Error subclass: #RegistroVacioError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Error subclass: #TransformacionNoValidaError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #VisorNuevo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!VisorNuevo methodsFor: 'not defined category' stamp: 'MartinAlderete 5/8/2024 17:25'!calcularKiSegunModelo: unKi	^ unKi.! !Object subclass: #VisorViejo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!VisorViejo methodsFor: 'not defined category' stamp: 'MartinAlderete 5/8/2024 18:20'!calcularKiSegunModelo: unKi	|kiLimite kiCalculado|	kiLimite := 9000.		kiCalculado := unKi min: kiLimite.			^ kiCalculado.! !